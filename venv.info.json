{
    "venv": {
        "type": "conda",
        "name": "hodsa",
        "python": "3.12"
    },
    "deps": {
        "conda_deps": [
            "conda install jupyterlab",
            "conda install matplotlib",
            "conda install nbconvert",
            "conda install ipykernel --update-deps --force-reinstall"
        ],
        "pip_deps": ["python -m pip install 'nbqa[toolchain]'"]
    }
}
